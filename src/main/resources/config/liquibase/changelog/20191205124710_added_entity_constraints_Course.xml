<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Course.
    -->
    <changeSet id="20191205124710-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="course_sme"
                                 constraintName="fk_course_sme_course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

        <addForeignKeyConstraint baseColumnNames="sme_id"
                                 baseTableName="course_sme"
                                 constraintName="fk_course_sme_sme_id"
                                 referencedColumnNames="id"
                                 referencedTableName="profile"/>
        
        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="course"
                                 constraintName="fk_course_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

    </changeSet>
</databaseChangeLog>
